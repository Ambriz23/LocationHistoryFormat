{
    "$id": "https://locationhistoryformat.com/schemas/Semantic.schema.json",
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Semantic Location History",
    "description": "A Semantic Location History JSON file in a Google Takeout Location History extraction. Contains Semantic Location History information from the user's account. Typically this will be a file containing data for a single month, with a name such as `2021_JANUARY.json`.",
    "extra_wip": true,
    "required": [
        "timelineObjects"
    ],
    "properties": {
        "timelineObjects": {
            "$id": "#/properties/timelineObjects",
            "type": "array",
            "title": "Timeline Objects",
            "description": "List of all available semantic information, in chronological order. Each item in the list is either an ![#/$defs/activitySegment] or a ![#/$defs/placeVisit], encapsulated in a generic ![#/properties/timelineObjects/items].",
            "items": {
                "type": "object",
                "title": "Timeline Object",
                "description": "Encapsulates either an ![#/$defs/activitySegment] or a ![#/$defs/placeVisit], depending on the single key found.",
                "oneOf": [
                    {
                        "type": "object",
                        "properties": {
                            "activitySegment": {
                                "$ref": "#/$defs/activitySegment"
                            }
                        }
                    },
                    {
                        "type": "object",
                        "properties": {
                            "placeVisit": {
                                "$ref": "#/$defs/placeVisit"
                            }
                        }
                    }
                ]
            }
        }
    },
